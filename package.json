{
  "name": "react-webpack-boilerplate",
  "version": "1.0.0",
  "description": "Miminalistic starter template for server-side-renderd react applications",
  "main": "src/index.js",
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "start": "nodemon src/index.js --ignore src/client --ignore src/css --exec babel-node src/index.js",
    "prestart": "npm run css",
    "lint": "eslint src --color",
    "test": "jest",
    "pretest": "eslint src --color",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "css": "tachyons src/css/app.css > src/index.css --minify",
    "build": "webpack --env prod --config webpack.prod.js && babel src -d dist"
  },
  "not-yet-used": [
    "flow",
    "prettier"
  ],
  "keywords": [
    "tachyons",
    "functional-css",
    "express",
    "docker",
    "SSR",
    "react",
    "server-side",
    "webpack",
    "HMR"
  ],
  "author": "the",
  "license": "ISC",
  "dependencies": {
    "classnames": "~2.2.6",
    "cookie-parser": "~1.4.3",
    "express": "~4.16.3",
    "express-handlebars": "~3.0.0",
    "http-graceful-shutdown": "~2.1.1",
    "prop-types": "~15.6.2",
    "react": "~16.4.1",
    "react-dom": "~16.4.1",
    "react-popper": "~1.0.0",
    "react-router-dom": "~4.3.1",
    "superagent": "~3.8.3",
    "superagent-use": "~0.1.0"
  },
  "devDependencies": {
    "babel-cli": "~6.26.0",
    "babel-core": "~6.26.3",
    "babel-eslint": "~8.2.5",
    "babel-loader": "~7.1.5",
    "babel-preset-env": "~1.7.0",
    "babel-preset-react": "~6.24.1",
    "copy-webpack-plugin": "~4.5.2",
    "css-loader": "~1.0.0",
    "eslint": "~5.1.0",
    "eslint-config-airbnb": "~17.0.0",
    "eslint-plugin-react": "~7.10.0",
    "file-loader": "~1.1.11",
    "jest": "~23.3.0",
    "mini-css-extract-plugin": "~0.4.1",
    "nodemon": "~1.17.5",
    "optimize-css-assets-webpack-plugin": "~4.0.3",
    "style-loader": "~0.21.0",
    "tachyons": "~4.10.0",
    "tachyons-cli": "~1.3.0",
    "uglifyjs-webpack-plugin": "~1.2.7",
    "url-loader": "~1.0.1",
    "webpack": "~4.15.1",
    "webpack-cli": "~3.0.8",
    "webpack-dev-middleware": "~3.1.3",
    "webpack-hot-client": "~4.0.4",
    "webpack-merge": "~4.1.3"
  }
}
